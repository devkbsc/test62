{% extends 'base.html.twig' %}

{% block title %}Attribution
{% endblock %}

{% block body %}
	<div class="container-fluid">
		<h1>Attribution</h1>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ attribution.id }}</td>
				</tr>
				<tr>
					<th>NoPoint</th>
					<td>{{ attribution.noPoint }}</td>
				</tr>
				<tr>
					<th>DateAdded</th>
					<td>{{ attribution.dateAdded ? attribution.dateAdded|date('Y-m-d H:i:s') : '' }}</td>
				</tr>
			</tbody>
		</table>

		<a href="{{ path('app_attribution_index') }}">back to list</a>

		<a href="{{ path('app_attribution_edit', {'id': attribution.id}) }}">edit</a>

		{{ include('attribution/_delete_form.html.twig') }}
	</div>
{% endblock %}
